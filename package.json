  "license": "MIT",
  "packageManager": "pnpm@10.7.0",
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@changesets/cli": "^2.29.4",
    "@eslint/js": "^9.27.0",
    "@eslint/json": "^0.12.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.15.21",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.27.0",
    "globals": "^16.2.0",
    "jiti": "^2.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.41.1",
    "rollup-plugin-dts": "^6.2.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
