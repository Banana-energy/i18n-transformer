{
  "version": "2.0.0",
  "description": "An automatic i18n conversion Vite/Webpack plugin.",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm -r clean && pnpm -r build ",
    "test": "vitest run",
