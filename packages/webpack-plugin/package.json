{
  "name": "@higgins/webpack-plugin-i18n-transformer",
  "version": "1.1.0",
  "description": "webpack-plugin-i18n-transformer",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "./dist/utils": {
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs"
    },
    "./utils/*": {
      "import": "./dist/utils/*.mjs",
      "require": "./dist/utils/*.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": "Banana-energy",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf dist"
  },
  "release-it": {
    "git": false,
    "plugin": {
      "@release-it/bumper": {
        "out": {
          "file": "package.json",
          "path": [
            "dependencies.@karpo/shared"
          ]
        }
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@higgins/shared": "workspace:*",
    "@rollup/pluginutils": "^5.1.2"
  },
  "devDependencies": {
    "@release-it/bumper": "^6.0.1",
    "release-it": "^17.7.0",
    "webpack": "^5.95.0"
  }
}
